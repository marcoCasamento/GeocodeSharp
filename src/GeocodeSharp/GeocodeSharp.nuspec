    </releaseNotes>
    <copyright>Copyright 2014</copyright>
    <tags>Geocode Address Maps Geo</tags>
    <dependencies>
      <group targetFramework=".NETFramework4.5.1">
        <dependency id="Newtonsoft.Json" version="6.0.5" />
      </group>
      <group targetFramework=".NETStandard1.6">
        <dependency id="Newtonsoft.Json" version="9.0.1" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src=".\bin\$configuration$\net451\GeocodeSharp.dll" target="lib\net451\GeocodeSharp.dll" />
    <file src=".\bin\$configuration$\netstandard1.6\GeocodeSharp.dll" target="lib\netstandard1.6\GeocodeSharp.dll" />
  </files>
</package>
