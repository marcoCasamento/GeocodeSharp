<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>GeocodeSharp</id>
    <version>$version$</version>
    <title>Async Google Geocode API for .NET</title>
    <authors>Brandon Cuff, Marco Casamento</authors>
    <owners>Brandon Cuff</owners>
    <licenseUrl>https://github.com/bcuff/GeocodeSharp/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/marcocasamento/GeocodeSharp</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Simple and lightweight wrapper around Google geocoding api</description>
    <releaseNotes>
      1.6.0 Switched GeocodeClient implementation from WebRequest to HttpClient
